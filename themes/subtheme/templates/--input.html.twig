
{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */
#}
{% if element['#type'] == 'submit' %}
<input{{ attributes }} />{{ children }}
{% elseif element['#type'] == 'checkbox' %}
<div class="checkbox-empty"><div class="checkbox-active"></div></div>
<input{{ attributes.addClass('w-checkbox-input checkmark-tick').setAttribute('data-ix', 'check-box-click') }} />{{ children }}
{% elseif element['#type'] == 'radio' %}
<input{{ attributes }} />{{ children }}
{% else %}
<input{{ attributes.addClass('w-input form-field') }} />{{ children }}
{% endif %}
