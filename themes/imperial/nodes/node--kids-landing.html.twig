<div data-node="kids_landing">
    <div class="full-width-section">
      <div class="img-overlay-35"></div>
      <div class="header-img-lg object-fit">
        {{ content.kids_landing_background }}</div>
      <div class="_8col container position-absolute txt-centered valign-parent">
        {{ content.kids_landing_overlay }}<div class="valign">
          <h1 class="tertiary" data-node-field="title">{{ content.kids_landing_title }}</h1>
          <div class="body-lg tertiary" data-node-field="description">{{ content.kids_landing_description }}</div>
        </div>
      </div>
    </div>
    <div class="bg-secondary default-section">
      <div class="container w-clearfix">
        <div class="_66-pc" data-view="kids_popular_recipe" data-filter-type="node" data-filter-bundle="kids_kitchen" data-node-field="popular_recipe" data-target-type="view" data-target-bundle="kids_popular_recipe">{{ content.kids_landing_popular_recipe }}</div>
