<?php

/**
 * @file
 * Contains ck_editor_button.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function ck_editor_button_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the ck_editor_button module.
    case 'help.page.ck_editor_button':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}
